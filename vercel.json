{
  "builds": [
    {
      "src": "*.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "App/app_init_.py": {
      "includeFiles": [
        "Rootes/**"
      ]
    },
    "App/Rootes/rootes.py": {
      "includeFiles": [
        "App/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}